<div id="link">
 <h3><%= link_to 'Create Inquiry', new_inquiry_path %> ||
 <%= link_to 'All Inquiry', inquiries_path %></h3>
</div>
<h1>All Inquiry</h1>

<table>
  <tr>
    <th>Date</th>
    <th>System</th>
    <th>Company</th>
 
    <th>E mail</th>
    
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @inquiries.each do |inquiry| %>
  <tr>
    <td><%= inquiry.date %></td>
    <td><%= inquiry.system %></td>
    <td><%= inquiry.company %></td>
   
    <td><%= inquiry.e_mail %></td>
   
    <td><%= link_to 'View', inquiry %></td>
    <td><%= link_to 'Assign to:', edit_inquiry_path(inquiry) %></td>
    <!--<td><%= link_to 'Destroy', inquiry, :confirm => 'Are you sure?', :method => :delete %></td>-->
  </tr>
<% end %>
</table>

